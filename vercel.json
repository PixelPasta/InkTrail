{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@now/node"
      }
    ],
    "routes": [
        {
            "src": "/public/(.*)", 
            "dest": "/public/$1"   
          },
      {
   
        "src": "/(.*)",
       
        "dest": "index.js"
      }
    ]
  }